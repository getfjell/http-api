{
  "name": "@fjell/http-api",
  "description": "HTTP API for Fjell",
  "version": "4.4.21",
  "keywords": [
    "http",
    "api",
    "fjell"
  ],
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "concurrently \"tsc --noEmit --watch\" \"node build.js\"",
    "build": "pnpm run lint && tsc --noEmit && node build.js",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rimraf dist",
    "test": "pnpm run lint && vitest run --coverage",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview",
    "docs:test": "cd docs && npm run test"
  },
  "dependencies": {
    "@fjell/logging": "^4.4.29"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@fjell/eslint-config": "^1.1.2",
    "@tsconfig/recommended": "^1.0.10",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.31.0",
    "jsdom": "^26.1.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "undici": "^7.12.0",
    "vitest": "^3.2.4",
    "vitest-fetch-mock": "^0.4.5"
  },
  "packageManager": "pnpm@10.13.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/http-api.git"
  }
}
